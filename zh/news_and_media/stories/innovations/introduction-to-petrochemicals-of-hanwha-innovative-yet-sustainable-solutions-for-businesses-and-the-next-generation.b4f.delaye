<script type="text/javascript">
var statusCode = "";
var lang = "zh";
var agree = false;
var focusTarget = "";
var isMobile = "false";


function check_agree() {

	agree = true;
}

function checkHasSubscribe() {

	var ajaxPath = "/content/hanwha/" + lang + "/contact/subscribe/_jcr_content/parsys/content_container/box_inner/subscribe.checkSubscribed.json";
	var hasSubscribe = false;

	console.log("submit_subscribe called!!!");
	console.log("ajaxPath: " + ajaxPath);

	$.ajax({

		type: "GET",
		url: ajaxPath,
		data: {
			"lang"         : lang,
			"email"        : $('#inp-search').val()
		},
		dataType: "json",
		success: function(result) {

			console.log("ajax success!!!");
//				alert("ajax success!!!");

			if(result.success == "true") {

				if(result.subscribe == "true") {

					hasSubscribe = true;
				}

				if(hasSubscribe) {

					console.log("has subscribe!!!");
//						alert("has subscribe!!!");
					$(".alertLayer07").trigger("click");
					return false;
				} else {

					console.log("no subscribe!!!");
//						alert("no subscribe!!!");
					submit_subscribe();
					return false;
				}
			}
		},
		error: function(rlt) {

			console.log("ajax failed!!!");
//				alert("ajax failed!!!");
		}
	});
}

function submit_subscribe() {

	var strEmail            = $("#inp-search").val();
	var targets				= "Aerospace & Mechatronics, Chemicals & Materials, Construction, Financial Services, Leisure & Lifestyle, Solar Energy, Corporate News";
	var flag 				="newsroom_content";

	if ( strEmail == null || strEmail == "" ) {

		$(".alertLayer01").trigger("click");
		focusTarget = $("#inp-search");
		return false;
	} else {

		var re = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/;

		if ( !re.test(strEmail)) {

			$(".alertLayer02").trigger("click");
			focusTarget = $("#chkEmail");
			return false;
		} else {

			$("#subscribe_db_email").val(strEmail);
			$("#subscribe_db_ag").val(agree);
			$("#subscribe_db_targ").val(targets);
			$("#subscribe_db_flag").val(flag);
			$("#subscribe_db").submit();
		}
	}

	return false;
}

function go_Share(sName) {

	var oriName = ".at-svc-"+sName;
	oriName += " > .at-icon-wrapper";
	$(oriName).trigger('click');
	$("#btnShareClose").trigger('click');
}

function changeParam() {

	var tags ="韩华道达尔, 太阳能, 石化产品, 高新材料";
	var tag = tags.split(",");
	var str="";

	for(var i=0;i<tag.length;i++) {

		var encode_text = encodeURIComponent(tag[i].trim());
		str +="<li><a href=\"/"+ lang +"/tag.html?find="+ encode_text +"\" target=\"_blank\" onclick=\"dtmDataLayer.internallink='Tag List Click("+tag[i].trim()+")'; _satellite.track('internallink');\">"+ tag[i].trim() + "</a></li>";
	}

	$(".list_tag").html(str);
}

$(function() {

	$('.submit_area a').each(function() {

		$(this).click(function() {

			var footLeft = ($(window).width() - $('.layer_map_info').outerWidth()) /2;
			var footTop = ($(window).outerHeight() - $('.layer_map_info').outerHeight()) /2;
			var layerId= $(this).attr('href');

			$('.mask_layer').fadeIn(350,'swing',function() {

				$(this).siblings(layerId).css({
					left: footLeft,
					top: footTop
				}).fadeIn(350);

				return false;
			});

			$(".closeLayer").bind('click',function() {

				$('.layer_map_info').fadeOut(350,'swing',function() {

					$('.mask_layer').fadeOut(350);
				});

				$('body').css({
					overflow: 'auto',
					paddingRight: 0
				});

				if ( statusCode == "" ) {

					focusTarget.focus();
				}

				statusCode = "";
				return false;
			});

			$('.close_layer').bind('click',function() {

				$('.layer_map_info').fadeOut(350,'swing',function() {

					$('.mask_layer').fadeOut(350);
				});

				$('body').css({
					overflow: 'auto',
					paddingRight: 0
				});

				if ( statusCode == "" ) {

					focusTarget.focus();
				}

				statusCode = "";
				return false;
			});

			$('.mask_layer').click(function() {

				$('.layer_map_info').fadeOut(350,'swing',function() {

					$('.mask_layer').fadeOut(350);
				});

				$('body').css({
					overflow: 'auto',
					paddingRight: 0
				});

				if ( statusCode == "" ) {

					focusTarget.focus();
				}

				statusCode = "";
				return false;
			});
		});
	});
});
</script>



