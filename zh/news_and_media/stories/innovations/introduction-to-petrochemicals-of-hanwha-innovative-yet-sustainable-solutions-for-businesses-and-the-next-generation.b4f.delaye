<script type="text/javascript">

    //쿠키 세팅 함수
    //cname : 쿠키에 저장할 변수 이름 / cvalue : 쿠키에 저장할 값 / exdays : 쿠키소멸기한
    function setCookie(cname,cvalue,exdays){
        //쿠키 세팅 전 countryCode 쿠키를 소멸시킴 ( countryCode의 값을 없앰 / exdays = -1 시 쿠키 소멸)

        /* 소멸 기한을 일 기준으로 잡기 위해 날짜 세팅 	:: 시작 */
        var date = new Date();
        date.setTime(date.getTime() + (exdays*24*3600*1000));
        var expires = "expires="+date.toUTCString();
        /* 소멸 기한을 일 기준으로 잡기 위해 날짜 세팅	:: 끝 */

        //매개변수의 값들로 쿠키에 저장
        document.cookie = cname+ "=" + cvalue+";" + expires;
    }

    //쿠키 가져오는 함수
    function getCookie(cname){
        var name = cname+"=";

        //저장된 쿠키를 구분자 ';'를 이용해 split
        var cookieArr = document.cookie.split(';');

        //split된 값들의 length만큼 돔
        for(var i=0; i<cookieArr.length; i++){
            var cookie = cookieArr[i];
            //split된 쿠키에서 공백 제거
            while(cookie.charAt(0)==' '){
                cookie = cookie.substring(1);
            }
            //쿠키의 name이 있을 시 쿠키에서 name을 잘라냄 (countryCode=abc -> abc)
            if(cookie.indexOf(name) != -1){
                //쿠키값 리턴
				return cookie.substring(name.length,cookie.length);
            }
        }
        //없을 시 빈값 리턴
        return "";
    }

    //www.hanwha.com체크
    var directCheck = "";
    //현재 보고있는 페이지 언어 체크
    var pageLanguage = "zh";
    //쿠키에 저장된 값 체크
    var cookieLanguage = getCookie("countryCode");

    //현재 브라우저 언어 저장
	var browserLanguage ="";

    //navigator.language가 작동하는지 체크 (IE에서는 navigator.userLanguage 가 작동됨)
    if(navigator.language==undefined){
        // 브라우저의 언어를 체크 할 수 없을 때 default로 en 저장
        if(navigator.userLanguage ==undefined){
            browserLanguage ="en";
        }
        //IE 언어 저장
        else{
            browserLanguage = navigator.userLanguage;
        }
        //나머지 브라우저 언어 저장
    }else{
		browserLanguage = navigator.language;
    }

    //언어코드가 (en-us 등과 같은 값일 경우 문자열 잘라서 사용)
    browserLanguage = browserLanguage.substr(0,2);

    //브라우저 언어가 사이트에서 지원하는 언어가 아닐시 default en으로 저장
    if(browserLanguage !="zh" && browserLanguage !="ja" && browserLanguage !="de" && browserLanguage !="es"){
        browserLanguage = "en";
    }

    var host = document.location.host;
    var pathname = document.location.pathname;
    var port = document.location.port;

    var refer = document.referrer;

    //메인에서 만 쿠키를 활용위해 저장
    var depth ="7";

    // 레퍼러가 메인일 경우
    if(refer.indexOf("/en.")>0){
     	refer="en";
    }else if(refer.indexOf("/es.")>0){
     	refer="es";
    }else if(refer.indexOf("/de.")>0){
     	refer="de";
    }else if(refer.indexOf("/ja.")>0){
     	refer="ja";
    }else if(refer.indexOf("/zh.")>0){
     	refer="zh";
    }else{
        //레퍼러가 서브페이지 일 경우
      	refer=refer.substr(refer.indexOf("/"+pageLanguage+"/")+1,2);
    }

    var link = "";

    //포트체크
    if(directCheck=="u"){
        //if(port==""){
            //같은 페이지 리다이렉트 방지
            if(document.referrer != document.location.href){
                //로고버튼 리다이렉트 방지
                if(pageLanguage != refer){
                    //메인에서만 쿠키활용
                    if(depth==3){
                        //쿠키 있을때 처리(브라우저 언어)
                        if(cookieLanguage !=""){
                            if(port == ""){
                                link = 'https://'+host+'/'+cookieLanguage+'.html';
                            }else{
                                link = 'http://'+host+'/content/hanwha/'+cookieLanguage+'.html';
                            }
                            setCookie("countryCode",cookieLanguage,30);
                            if(document.location.href != link){
                                document.location.href=link;
                            }

                        //쿠키 없을때 처리
                        }else{
                            if(port == ""){
                                link = 'https://'+host+'/'+pageLanguage+'.html';
                            }else{
                                link = 'http://'+host+'/content/hanwha/'+pageLanguage+'.html';
                            }
                            if(document.location.href != link){
                                document.location.href=link;
                            }
                        }
                    }
                }
            }
        //}
    }

</script>
	





