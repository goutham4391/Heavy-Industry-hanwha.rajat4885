<script type="text/javascript">

    function searchText(){
        var re = /[\{\}\[\]\/?,;:|\)*~`!^\-_¦«<>#\'\"\\(\=]/gi;
        var searchText = $("#searchAll").val();
        var reqUrl = "hanwha-systems-acquired-the-business-and-assets-of-phasor-solutions-a-uk-based-satellite-communication-antenna-developer-to-step-up-i";



       	if(searchText.length<2){
       		alert("Please enter at least 2 characters.");
   			return;
       	}
       	//특수문자 얼럿
       	var aSpecialChar = ["\"", "'", "+", "../../../index.html", ":", ";", "\\", "&", "<" , ">"];
        var eKey = false;

        var sKey = searchText;
        for (var i = 0 ; i < sKey.length ; i++) {
            for (var j = 0 ; j < aSpecialChar.length ; j++) {
                if (aSpecialChar[j] == sKey.charAt(i)) {
                    eKey = true;
                }
            }
        }
        if (eKey) {
            alert("The keyword contains an incorrect string \n [\" , \' , \/ , ; , : , \\]");
            return false;
        }

       	searchText = searchText.trim();
        searchText = searchText.replace(re, "");
        searchText = searchText.replace("        ", " ");
        searchText = searchText.replace("       ", " ");
        searchText = searchText.replace("      ", " ");
        searchText = searchText.replace("     ", " ");
        searchText = searchText.replace("    ", " ");
        searchText = searchText.replace("   ", " ");
        searchText = searchText.replace("  ", " ");
        //searchText = searchText.replace(/[ ]/gi, "+");
        searchText = searchText.replace(/[ ]/gi, " ");

        searchText = searchText.replace("\\^", "");
    	searchText = searchText.replace("\\{", "");
  	  	searchText = searchText.replace("\\}", "");
   	 	searchText = searchText.replace("\\(", "");
    	searchText = searchText.replace("\\)", "");
    	searchText = searchText.replace("\\[", "");
    	searchText = searchText.replace("\\]", "");


        searchText = encodeURIComponent( searchText );
        if(reqUrl.indexOf("hanwha.com")>=0){
        searchText = encodeURIComponent( searchText );
        }
    	/*  if(searchText.indexOf("%")>=0){
        	searchText = searchText.replace(/%/g,escape("%"));
        }    */

        var locationpath = "../../search.html";

        location.href = locationpath + "?cate=all&find="+searchText;


    }
</script>


	





