				$('#footer address img').attr('src','../../../content/dam/hanwha/resources/images/common/img_logo_footer.png');
            }else{
				if(depth > 5 || pageName == "Greenfootprintproject2021" || pageName == "newslette-subscription" ){
					$('#gnb h1 a img').attr('src','../../../content/dam/hanwha/resources/images/common/img_logo_white.svg');//2019-12-10 (PC: 뎁스3 일때만 적용_분기처리) img_logo_white 로 사용
				}else{
					$('#gnb h1 a img').attr('src','../../../content/dam/hanwha/resources/images/common/img_logo.svg'); //2019-12-10 뎁스1,2에서 사용되는 이미지
				}
				$('#footer address img').attr('src','../../../content/dam/hanwha/resources/images/common/img_logo_footer.svg');
            }

		});
	</script>

