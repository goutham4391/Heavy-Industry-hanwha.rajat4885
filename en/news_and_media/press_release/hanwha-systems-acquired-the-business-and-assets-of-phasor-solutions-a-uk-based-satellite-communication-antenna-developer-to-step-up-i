<script>
    function videopercent() {
    	try {
            var controller = new VgControllerClient({
                target_window: document.getElementById('klus_vdo').contentWindow
            });

            console.log('이벤트 준비');
    		window.onbeforeunload = function() {
                dtmDataLayer.videopercent=controller.get_progress().percent;
                _satellite.track('videopercent');
                console.log('이벤트 성공'+controller.get_progress().position);
            };
        } catch(e) {
		}
    };
</script>

