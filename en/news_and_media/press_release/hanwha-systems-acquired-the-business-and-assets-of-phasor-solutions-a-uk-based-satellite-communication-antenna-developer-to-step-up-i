	function speakText(){
		var polly = new AWS.Polly();
		var params ={
				OutputFormat :"mp3",
				Text:"",
				TextType:"text",
				VoiceId: "",
				Engine:"",
				LanguageCode:"",
				OutputS3BucketName :"hanwha-polly-audio-files",
				OutputS3KeyPrefix :lang+"-hanwha-systems-acquired-the-business-and-assets-of-phasor-solutions-a-uk-based-satellite-communication-antenna-developer-to-step-up-its-advance-into-the-global-business"
		};

		var voiceType=$('input:radio[name="voice"]:checked').val();


		params.VoiceId =voiceType;
		params.Text =$("#aws_polly").text();
		if(lang =="zh"){
			params.LanguageCode ="cmn-CN";
			params.VoiceId ="Zhiyu";
			params.Engine ="standard"; //중문은 신경망 voice 없음

		}else{
			params.LanguageCode ="en-US";
			params.Engine ="neural";
		}

// 		polly.synthesizeSpeech(params, function(err, data){
// 			if(err){
// 				$("#result").text(err);
// 			}else{

// 				var uInt8Array = new Uint8Array(data.AudioStream);
// 				var arrayBuffer = uInt8Array.buffer;
// 				var blob = new Blob([arrayBuffer]);
// 				var url = URL.createObjectURL(blob);
// 				$("#audioSource").attr('src',url);
// 				$("#audioPlayback").load();
// 				$("#result").text("play");
// 			}
// 		});

		polly.startSpeechSynthesisTask(params, function(err, data){
			if(err){
				console.log("FAILED!");
				//$("#result").text(err);
			}else{
				var url=data.SynthesisTask.OutputUri;
				console.log("makingURL::"+ url);

				setTimeout(function() {
					$("#audioSource").attr('src',url);
	 				//$("#audioPlayback").load();
	 				audioPolly();
	 				console.log("SUCCESS!");
					}, 20000);


			}
		});
	}

	function getBklist(){
		var s3 = new AWS.S3();
		var params = {
				Bucket: 'hanwha-polly-audio-files',
				MaxKeys : 1000
				};

		var currentName = lang+"-hanwha-systems-acquired-the-business-and-assets-of-phasor-solutions-a-uk-based-satellite-communication-antenna-developer-to-step-up-its-advance-into-the-global-business";
		var pollyUrl ="";
		var preUrl="https://hanwha-polly-audio-files.s3.ap-northeast-2.amazonaws.com/";

		s3.listObjectsV2(params, function(err, data) {
		  if (err) console.log(err, err.stack);
		  else{

			  $.each(data.Contents , function (index, item){
				  //console.log("@@:: "+item.Key);

				  if(item.Key.indexOf(currentName) > -1){
					  pollyUrl = item.Key;
					  return false;
				  }

			  });

			  if(pollyUrl !=""){
				  //$("#select_polly").hide();
				  //console.log("PollyURL::"+pollyUrl);



				  $("#audioSource").attr('src',preUrl+pollyUrl);
                  //$("#audioPlayback").load();
	 			  audioPolly();
	 			 $(".audio_pollywrap").show();
			  }else{
				  if(isAuthor){
					  $("#select_polly").show();
				  }
				  $(".audio_pollywrap").hide();
			  }
		  }
		});

	}

    $("#pausenow").click(function(){
        $("#audioPlayback").get(0).pause();
        console.log("videos pause");
        $("#result").text("videos pause");
	});

    $("#stopnow").click(function(){
        $("#audioPlayback").get(0).pause();
        $("#audioPlayback").get(0).currentTime=0;
        console.log("videos stop");
        $("#result").text("videos stop");
	});

	$("#playnow").click(function(){
        $("#audioPlayback").get(0).play();
        console.log("videos play");
        $("#result").text("videos play");
	});

	$("#createPolly").click(function(){
        console.log("createpolly");
		speakText();
	});

	$("#testBtn").click(function(){
		console.log("TEXT\n"+$("#aws_polly").text());
		getBklist();
	});


	</script>
